<?xml version="1.0"?>

<layout xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/layout_generic.xsd">
    <referenceContainer name="content">
        <block class="MageDev\AutoSearch\Block\Result"
               name="auto.search.result">
            <block class="Magento\CatalogSearch\Block\SearchResult\ListProduct"
                   name="search_result_list">
                <arguments>
                    <argument name="positioned" xsi:type="string">positions:list-secondary</argument>
                </arguments>
            </block>
        </block>
    </referenceContainer>
</layout>